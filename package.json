{
  "name": "roady-2.0",
  "version": "1.0.0",
  "description": "A roadtrip music discovery app",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev:nodemon": "NODE_ENV=development node_modules/.bin/nodemon --ignore '/app/' -e js server -L",
    "dev:build": "NODE_ENV=development webpack --watch",
    "dev:server": "NODE_ENV=development node server",
    "dev:debug": "NODE_ENV=development node --debug server",
    "prod:build": "NODE_ENV=production webpack",
    "prod:server": "NODE_ENV=production node server"
  },
  "jest": {
    "setupFiles": [
      "./app/__test__/shim.js",
      "./app/__test__/jestsetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/__test__/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/app/__test__/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marshalldanel/roady-2.0.git"
  },
  "author": "Marshall",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marshalldanel/roady-2.0/issues"
  },
  "homepage": "https://github.com/marshalldanel/roady-2.0#readme",
  "dependencies": {
    "apollo-boost": "^0.1.4",
    "apollo-server-express": "^1.3.6",
    "body-parser": "^1.18.2",
    "bulma": "^0.7.1",
    "css-loader": "^0.28.4",
    "dotenv": "^5.0.1",
    "express": "4.16.3",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "graphql-tools": "^2.24.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.2",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.9.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-addons-shallow-compare": "^15.6.0",
    "react-apollo": "^2.1.3",
    "react-dates": "^16.6.1",
    "react-dom": "^16.3.2",
    "react-places-autocomplete": "^7.0.0",
    "sass-loader": "^7.0.1",
    "scriptjs": "^2.5.8",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-hot-middleware": "^2.22.1"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "compression-webpack-plugin": "^1.1.11",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.2.1",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^22.4.3",
    "nodemon": "^1.17.3",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.15"
