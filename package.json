{
  "name": "roady-2.0",
  "version": "1.0.0",
  "description": "A roadtrip music discovery app",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev:nodemon": "NODE_ENV=development node_modules/.bin/nodemon -e js server -L",
    "dev:build": "NODE_ENV=development webpack",
    "dev:server": "NODE_ENV=development node server",
    "dev:debug": "NODE_ENV=development node --debug server",
    "prod:build": "NODE_ENV=production webpack -p",
    "prod:server": "NODE_ENV=production node server"
  },
  "jest": {
    "setupFiles": [
      "./app/__test__/shim.js",
      "./app/__test__/jestsetup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/app/__test__/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/app/__test__/__mocks__/styleMock.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marshalldanel/roady-2.0.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/marshalldanel/roady-2.0/issues"
  },
  "homepage": "https://github.com/marshalldanel/roady-2.0#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bulma": "^0.6.2",
    "css-loader": "^0.28.8",
    "dotenv": "^4.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "express": "^4.16.2",
    "html-webpack-plugin": "^2.30.1",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "node-fetch": "^1.7.3",
    "node-sass": "^4.7.2",
    "querystring": "^0.2.0",
    "react": "^16.2.0",
    "react-dates": "^16.0.2",
    "react-dom": "^16.2.0",
    "react-places-autocomplete": "^5.4.3",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-immutable-state-invariant": "^2.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
